<!DOCTYPE html>
<html lang="en">

<head>
   <%- include('../partials/head'); %>
</head>

<body class="container">

   <header>
      <%- include('../partials/header'); %>
   </header>

   <main>
      <div class="jumbotron">
         <h2>DANH SÁCH CÁC CUỘC BỎ PHIẾU ĐÃ KẾT THÚC</h2>
         <br>
         <br>
         <% for(let item of list) { %>
            <h3>
               Tên cuộc bỏ phiếu: 
               <%= item.title %>
            </h3>
            
            <table class="table table-striped">
               <thead>
                  <tr>
                     <th scope="col">#</th>
                     <th scope="col">Tên ứng viên</th>
                     <th scope="col">Kết quả</th>
                  </tr>
               </thead>
               <tbody>
                  <% item.candidateusers.forEach((item2,index)=> { %>
                     <tr>
                        <th scope="row">
                           <%= index + 1%>
                        </th>
                        <td>
                           <%= item2.name %>
                        </td>
                        <td>
                           <%= item2.choose %>
                        </td>
                     </tr>
                     <% }); %>
               </tbody>
            </table>
               <% } %>
      </div>
   </main>

   <script>

   </script>
   <footer>
      <%- include('../partials/footer'); %>
   </footer>

</body>

</html>